{
    "method": "DELETE",
    "header": [
        {
            "key": "Authorization",
            "value": "Bearer {{token}}",
            "type": "text"
        },
        {
            "key": "Content-Type",
            "value": "application/json",
            "type": "text"
        },
        {
            "key": "x-tenant",
            "value": "test",
            "type": "default"
        }
    ],
    "url": {
        "raw": "{{host}}/types/{{type_name}}",
        "host": [
            "{{host}}"
        ],
        "path": [
            "types",
            "{{type_name}}"
        ]
    }
}