{
  "method": "PUT",
  "header": [
    {
      "key": "Authorization",
      "value": "Bearer {{token}}",
      "type": "text"
    },
    {
      "key": "Content-Type",
      "value": "application/json",
      "type": "text"
    },
    {
        "key": "x-tenant",
        "value": "test",
        "type": "default"
    }
  ],
  "body": {
    "mode": "raw",
    "options": {
      "raw": {
        "language": "json"
      }
    },
    "raw_json_formatted": {
      "position": 1,
      "schema": {
        "type": "string",
        "pattern": "^[a-z0-9].*$"
      }
    }
  },
  "url": {
    "raw": "{{host}}/dimension/clientId",
    "host": [
      "{{host}}"
    ],
    "path": [
      "dimension"
    ]
  }
}