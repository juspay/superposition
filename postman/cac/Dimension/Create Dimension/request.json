{
  "method": "POST",
  "header": [
    {
      "key": "Authorization",
      "value": "Bearer {{token}}",
      "type": "text"
    },
    {
      "key": "Content-Type",
      "value": "application/json",
      "type": "text"
    },
    {
        "key": "x-tenant",
        "value": "test",
        "type": "default"
    },
    {
      "key": "x-org-id",
      "value": "{{org_id}}",
      "type": "default"
    }
  ],
  "body": {
    "mode": "raw",
    "options": {
      "raw": {
        "language": "json"
      }
    },
    "raw_json_formatted": {
      "dimension": "clientId",
      "position": 1,
      "schema": {
        "type": "string",
        "pattern": "^[a-z0-9].*$"
      }, 
      "description": "description",
      "change_reason": "change_reason"
    }
  },
  "url": {
    "raw": "{{host}}/dimension",
    "host": [
      "{{host}}"
    ],
    "path": [
      "dimension"
    ]
  }
}