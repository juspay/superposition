{
  "method": "PUT",
  "header": [
    {
      "key": "Authorization",
      "value": "Bearer {{token}}",
      "type": "text"
    },
    {
      "key": "Content-Type",
      "value": "application/json",
      "type": "text"
    },
    {
        "key": "x-tenant",
        "value": "test",
        "type": "default"
    },
    {
      "key": "x-org-id",
      "value": "{{org_id}}",
      "type": "default"
    }
    
  ],
  "body": {
    "mode": "raw",
    "options": {
      "raw": {
        "language": "json"
      }
    },
    "raw_json_formatted": {
      "override": {
        "key1": "value2"
      },
      "context": {
        "==": [
          {
            "var": "clientId"
          },
          "piyaz"
        ]
      }, 
      "description": "description",
      "change_reason": "change_reason"
    }
  },
  "url": {
    "raw": "{{host}}/context",
    "host": [
      "{{host}}"
    ],
    "path": [
      "context"
    ]
  }
}