{
  "method": "POST",
  "header": [
    {
      "key": "Authorization",
      "value": "Bearer {{token}}",
      "type": "text"
    },
    {
      "key": "x-tenant",
      "value": "{{x_tenant}}",
      "type": "default"
    },
    {
      "key": "Content-Type",
      "value": "application/json",
      "type": "text"
    }
  ],
  "body": {
    "mode": "raw",
    "options": {
      "raw": {
        "language": "json"
      }
    },
    "raw_json_formatted": {
      "admin_email": "test@gmail.com",
      "name": "testorg"
    }
  },
  "url": {
    "raw": "{{host}}/superposition/organisations",
    "host": [
      "{{host}}"
    ],
    "path": [
      "superposition",
      "organisations"
    ]
  }
}
