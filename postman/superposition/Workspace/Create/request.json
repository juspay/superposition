{
    "method": "POST",
    "header": [
      {
        "key": "Authorization",
        "value": "Bearer {{token}}",
        "type": "text"
      },
      {
        "key": "Content-Type",
        "value": "application/json",
        "type": "text"
      },
      {
        "key": "x-org-id",
        "value": "testorg",
        "type": "text"
      }
    ],
    "body": {
       "mode": "raw",
       "options": {
           "raw": {
           "language": "json"
        }
       },
       "raw_json_formatted": {
          "workspace_admin_email": "test@gmail.com",
          "workspace_name": "test"
       }
    },
    "url": {
      "raw": "{{host}}/workspaces",
      "host": [
        "{{host}}"
      ],
      "path": [
        "workspaces"
      ]
    }
  }